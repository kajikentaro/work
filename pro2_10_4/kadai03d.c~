//課題3c  2019/10/4 19D8104026J 可児憲太郎
#include<stdio.h>
#define countNum 10
double getY(double X){
  return X*(X*X-2);
}
double mainfunction(double firstNum){
  double k=firstNum,l;
  int i;
  printf("%.15lf %.15lf\n",k,getY(k));
  for(i=0;i<countNum;i++){
    l=(2*k*k*k)/(3*k*k-2);
    k=l;
    printf("%.15lf %.15lf\n",k,getY(k));
  }
  return k;
}
int main(void){
  double result06 =mainfunction(0.6);
  puts("");
  double result07 =mainfunction(0.7);
  puts("");
  double result09 =mainfunction(0.9);
  puts("初期値の収束先");
  printf("初期値:0.6-> %.15lf\n",result06);
  printf("初期値:0.7-> %.15lf\n",result07);
  printf("初期値:0.9-> %.15lf\n",result09);
  return 0;
}
