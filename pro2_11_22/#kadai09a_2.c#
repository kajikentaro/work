//課題07b(2) 2019/11/8(FRI) 19d8104026j 可児憲太郎
#include<stdio.h>
#include<stdlib.h>
int coin500;
int is1k(){
    double tmp=(rand()+1.0)/(RAND_MAX+1.0);
    if(tmp>0.4){
        return 1;
    }else{
        return 0;
    }
}
int passed_people(){
    int i;
    for(i=1;i<=20;i++){
        if(is1k()){
            coin500--;
        }else{
            coin500++;
        }
        if(coin500<=0){
            break;
        }
    }
    return i;
}
int main(void){
    int a;
    scanf("%d",&a);

    int i;
    int record[22]={};
    for(i=0;i<1000000;i++){
        coin500=a;
        int tmp=passed_people();
        //printf("%d ",tmp);
        record[tmp]++;
    }
    for(i=1;i<=21;i++){
      printf("%d %lf\n",i,(double)record[i]/1000000);
    }
}  
